<h1><%= title %></h1>
<ul>
  <% if (data) { data.forEach(function(val){%>
  <!-- prettier-ignore -->
  <li>
      <a href= <%= val.url %>><%= val.book.title %>: <%= val.book.imprint %> - </a>
      <% if (val.status === 'Available') {%> <span class="text-success"><%= val.status %></span><% }%>
      <% if (val.status === 'Maintenance'|| val.status === 'Reserved') {%> <span class="text-danger"><%= val.status %></span><% }%>
      <% if (val.status == 'Loaned') {%> <span class="text-warning"><%= val.status %></span><% }%>
      <% if (val.status != "Available") {%> <span> Due: (<%= val.due_back_formatted %>)</span><% }%>
  </li>
  <% })} else { %>
  <li>There are no book copies in this library.</li>
  <% }%>
</ul>
